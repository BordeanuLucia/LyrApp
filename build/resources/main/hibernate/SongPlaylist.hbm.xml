<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
        "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
    <class name="model.SongPlaylist" table="song_playlist" lazy="false">
        <id name="id" type="java.lang.Long" column="id">
            <generator class="increment"/>
        </id>
        <property name="songId" type="java.lang.Long" column="songId"/>
        <property name="playlistId" type="java.lang.Long" column="playlistId"/>
    </class>
</hibernate-mapping>