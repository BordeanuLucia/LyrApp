<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.Cursor?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.Pane?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>


<AnchorPane prefHeight="180.0" prefWidth="381.0" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="controller.ConfirmationController">
   <children>
      <Label layoutX="7.0" layoutY="28.0" text="Esti sigur ca vrei sa stergi melodia?">
         <font>
            <Font size="24.0" />
         </font>
      </Label>
      <VBox layoutX="7.0" layoutY="63.0" prefHeight="121.0" prefWidth="367.0">
         <children>
            <Label fx:id="titleLabel" alignment="TOP_CENTER" pickOnBounds="false" prefHeight="50.0" prefWidth="367.0" text="Fara titlu" textAlignment="CENTER" wrapText="true">
               <font>
                  <Font name="System Italic" size="24.0" />
               </font>
            </Label>
            <Pane prefHeight="66.0" prefWidth="367.0">
               <children>
                  <Label fx:id="yesButton" accessibleRole="BUTTON" layoutX="67.0" layoutY="6.0" onMouseClicked="#handleDeleteSong" style="-fx-border-color: black; -fx-label-padding: 0 3 0 2;" text="DA">
                     <font>
                        <Font name="System Bold" size="36.0" />
                     </font>
                     <cursor>
                        <Cursor fx:constant="HAND" />
                     </cursor>
                  </Label>
                  <Label fx:id="noButton" accessibleRole="BUTTON" layoutX="263.0" layoutY="6.0" onMouseClicked="#handleNotDeleteSong" style="-fx-border-color: black; -fx-label-padding: 0 3 0 2;" text="NU">
                     <font>
                        <Font name="System Bold" size="36.0" />
                     </font>
                     <cursor>
                        <Cursor fx:constant="HAND" />
                     </cursor>
                  </Label>
               </children>
            </Pane>
         </children>
      </VBox>
   </children>
</AnchorPane>
